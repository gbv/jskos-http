{
    "name": "gbv/jskos-http",
    "description": "JSKOS API implementation (server and client)",
    "keywords": ["SKOS"],
    "homepage": "http://gbv.github.io/jskos-http/",
    "type": "library",
    "license": "LGPL",
    "author": [
        {
            "name": "Jakob VoÃŸ",
            "email": "jakob.voss@gbv.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "gbv/jskos": "~0.3",
        "symfony/cache": ">=3.3.6",
        "symfony/yaml": ">=2.8.0",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.0",
        "php-http/discovery": "^1.0"
    },
    "suggest": {
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1",
        "php-http/guzzle6-adapter": ">=1.1.1",
        "php-http/mock-client": "^1.0",
        "phploc/phploc": "^4.0",
        "theseer/phpdox": "^0.10.1"
    },
    "autoload": {
        "psr-4": {
            "JSKOS\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests --coverage-clover build/logs/clover.xml"
        ],
        "doc": [
            "vendor/bin/phploc -q --log-xml=build/logs/phploc.xml --count-tests src tests",
            "vendor/bin/phpdox"
        ]
    }
}
